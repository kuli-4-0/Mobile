<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="musicianFilterVM" type="com.bangkit.elise.modules.musicianfilter.data.viewmodel.MusicianFilterVM"/>
    </data>
    <FrameLayout android:id="@+id/frameMusicianFilter"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumnbg"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900">
            <View
                android:background="@drawable/rectangle_bg_white_a700_87_radius_tl_38_tr_38"
                android:id="@+id/viewBg"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_732pxh"
                android:layout_marginBottom="@dimen/_22pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_58pxv" android:layout_width="@dimen/_347pxh"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearFilterbar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_8">
            <View
                android:background="@drawable/rectangle_bg_gray_400_7f_radius_2_5"
                android:id="@+id/viewRectangle"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_26pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFilter"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{musicianFilterVM.musicianFilterModel.txtFilter}"
                style="@style/txtDMSansbold25" tools:text="@string/lbl_filter"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumntipe"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTipe"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{musicianFilterVM.musicianFilterModel.txtTipe}"
                    style="@style/txtDMSansmedium16" tools:text="@string/lbl_tipe"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearButton"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <LinearLayout android:id="@+id/linearBtn"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnMusisi"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.51"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_11pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_musisi"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_1" tools:text="@string/lbl_musisi"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnEvent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_50pxh"
                            android:layout_weight="0.49"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_11pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_event"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_event"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGenrelist"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGenre"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{musicianFilterVM.musicianFilterModel.txtGenre}"
                    style="@style/txtDMSansmedium16" tools:text="@string/lbl_genre"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearGenre"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <LinearLayout android:id="@+id/linearGenremusic"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_3">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreOne"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.27"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_18pxh"
                            android:paddingStart="@dimen/_17pxh"
                            android:paddingTop="@dimen/_13pxv"
                            android:singleLine="true"
                            android:text="@{musicianFilterVM.musicianFilterModel.txtGenreOne}"
                            style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_singer"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_weight="0.37"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_18pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@{musicianFilterVM.musicianFilterModel.txtGenreTwo}"
                            style="@style/txtSolidRounded_3" tools:text="@string/lbl_orchestra"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreThree"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_weight="0.36"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_14pxh"
                            android:paddingStart="@dimen/_13pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@{musicianFilterVM.musicianFilterModel.txtGenreThree}"
                            style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_tradisional"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLocation"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_78pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{musicianFilterVM.musicianFilterModel.txtLocation}"
                style="@style/txtDMSansmedium16" tools:text="@string/lbl_location"/>
            <LinearLayout android:id="@+id/linearLocationbtn"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_9">
                <LinearLayout android:id="@+id/linearRowcamera"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.98"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCamera"
                        android:layout_height="@dimen/_45pxh"
                        android:layout_width="@dimen/_45pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_camera"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_camera"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNewYourkUSA"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.73"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{musicianFilterVM.musicianFilterModel.txtNewYourkUSA}"
                        style="@style/txtDMSansbold16_3" tools:text="@string/lbl_new_yourk_usa"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_119pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="@dimen/_4pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_indigo_a200"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_indigo_a200"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowselectpricera"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSelectpricera"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.62"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{musicianFilterVM.musicianFilterModel.txtSelectpricera}"
                    style="@style/txtDMSansmedium16" tools:text="@string/msg_select_price_ra"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_112pxh"
                    android:layout_weight="0.38"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{musicianFilterVM.musicianFilterModel.txtPrice}"
                    style="@style/txtDMSansbold18" tools:text="@string/lbl_20_120"/>
            </LinearLayout>
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imagePricerange"
                android:layout_height="@dimen/_66pxh"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_pricerange"
                tools:ignore="ContentDescription" tools:src="@drawable/img_pricerange"/>
            <LinearLayout android:id="@+id/linearButtonfilter"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnReset"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.41"
                    android:layout_width="0dp"
                    android:letterSpacing="0.05"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_reset"
                    android:textAllCaps="true"
                    style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_reset"/>
                <FrameLayout android:id="@+id/frameBtnapply"
                    android:layout_height="@dimen/_58pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_width="@dimen/_185pxh" style="@style/groupStylecornerRadius_4">
                    <ImageView android:id="@+id/imageRectangleOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_58pxh"
                        android:layout_width="@dimen/_185pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtContinue"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_18pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.05"
                        android:singleLine="true"
                        android:text="@{musicianFilterVM.musicianFilterModel.txtContinue}"
                        android:textAllCaps="true"
                        style="@style/txtDMSansmedium16_1" tools:text="@string/lbl_apply"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
