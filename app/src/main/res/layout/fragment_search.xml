<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchVM" type="com.bangkit.elise.modules.search.data.viewmodel.SearchVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSearch"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_32pxv"
        android:layout_marginTop="@dimen/_48pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnarrowleft"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearTitleBar2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHelloAshfak"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{searchVM.searchModel.txtHelloAshfak}"
                        style="@style/txtDMSansmedium24" tools:text="@string/lbl_search2"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSearch1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search_indigo_a200"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search_indigo_a200"/>
                <View android:background="@color/indigo_300"
                    android:id="@+id/lineLineFiftyFour"
                    android:layout_height="@dimen/_26pxv"
                    android:layout_marginStart="@dimen/_13pxh" android:layout_width="@dimen/_1pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSearchOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{searchVM.searchModel.txtSearchOne}"
                    style="@style/txtDMSansbold24" tools:text="@string/lbl_search"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearCard1"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_2">
                <FrameLayout android:id="@+id/frameStackOne"
                    android:layout_height="@dimen/_92pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_79pxh" style="@style/groupStyleorange_200cornerRadius">
                    <ImageView android:id="@+id/imageImageEightyOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxh"
                        android:layout_width="@dimen/_79pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_image81}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image81"/>
                    <FrameLayout android:id="@+id/frameStack"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxv" android:layout_width="@dimen/_79pxh">
                        <ImageView
                            android:id="@+id/imageImageEightyFive"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_92pxh"
                            android:layout_width="@dimen/_79pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_image85}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image85"/>
                        <ImageView android:id="@+id/imageImageEightySix"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_92pxh"
                            android:layout_width="@dimen/_79pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_image86}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image86"/>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearEventdesc1"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_57pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.67"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{searchVM.searchModel.txtTime}"
                        android:textAllCaps="true"
                        style="@style/txtDMSansmedium12" tools:text="@string/msg_1st_may_sat"/>
                    <TextView android:id="@+id/txtImGoingtoSh"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:text="@{searchVM.searchModel.txtImGoingtoSh}"
                        style="@style/txtDMSansmedium18_2" tools:text="@string/msg_a_virtual_eveni"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerSearch"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_search"/>
        </LinearLayout>
    </LinearLayout>
</layout>
