<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myProfileVM" type="com.bangkit.elise.modules.myprofile.data.viewmodel.MyProfileVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackbackground"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageBackground"
            android:layout_gravity="start"
            android:layout_height="@dimen/_812pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_background"
            tools:ignore="ContentDescription" tools:src="@drawable/img_background"/>
        <LinearLayout android:id="@+id/linearColumnbackgroundOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameMyProfile"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageBackgroundOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_812pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_background"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_background"/>
                <LinearLayout android:id="@+id/linearColumnmusicianname"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_109pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_109pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageEightyNine"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_96pxh"
                        android:layout_marginEnd="@dimen/_86pxh"
                        android:layout_marginStart="@dimen/_86pxh"
                        android:layout_width="@dimen/_96pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image89}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image89"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMusicianName"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_86pxh"
                        android:layout_marginStart="@dimen/_86pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{myProfileVM.myProfileModel.txtMusicianName}"
                        style="@style/txtDMSansmedium24" tools:text="@string/lbl_bayu_chandra"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowtextnumberfollo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_82pxh"
                        android:layout_marginStart="@dimen/_86pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumntextnumberfollo"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.57"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTextnumberfollo"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_17pxh"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@{myProfileVM.myProfileModel.txtTextnumberfollo}"
                                style="@style/txtDMSansmedium16" tools:text="@string/lbl_350"/>
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtFollowingmyprof"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@{myProfileVM.myProfileModel.txtFollowingmyprof}"
                                style="@style/txtDMSansbold14" tools:text="@string/lbl_following"/>
                        </LinearLayout>
                        <View android:background="@color/gray_303"
                            android:id="@+id/lineLineFiftyNine"
                            android:layout_height="@dimen/_32pxv"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_weight="0.01" android:layout_width="0dp"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumntextnumberfolloOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_weight="0.43"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTextnumberfolloOne"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_18pxh"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@{myProfileVM.myProfileModel.txtTextnumberfolloOne}"
                                style="@style/txtDMSansmedium16" tools:text="@string/lbl_346"/>
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtFollowers"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@{myProfileVM.myProfileModel.txtFollowers}"
                                style="@style/txtDMSansbold14" tools:text="@string/lbl_followers"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowaddVideo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowedit"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                            <ImageView android:id="@+id/imageEdit"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_marginBottom="@dimen/_16pxv"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_14pxv"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_edit"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtD"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_15pxv"
                                android:layout_marginEnd="@dimen/_16pxh"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_weight="0.82"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_5pxh"
                                android:singleLine="true"
                                android:text="@{myProfileVM.myProfileModel.txtD}"
                                style="@style/txtDMSansbold16" tools:text="@string/lbl_edit_profile"/>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnAddVideo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_14pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_14pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_add_video"
                            android:textAllCaps="false"
                            style="@style/btnRoundedOutline" tools:text="@string/lbl_add_video"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPopular"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{myProfileVM.myProfileModel.txtPopular}"
                        style="@style/txtDMSansmedium18" tools:text="@string/lbl_about_me"/>
                    <FrameLayout android:id="@+id/frameStackdescription"
                        android:layout_height="@dimen/_98pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_309pxh">
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtDescription"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_9pxh"
                            android:text="@{myProfileVM.myProfileModel.txtDescription}"
                            style="@style/txtDMSansbold16_1" tools:text="@string/msg_enjoy_your_favo"/>
                        <ImageView android:id="@+id/imageArrowup"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_2pxh"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_120pxh"
                            android:layout_marginStart="@dimen/_120pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_5pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowup"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowup"/>
                    </FrameLayout>
                    <LinearLayout
                        android:id="@+id/linearRowavailableforh"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAvailableforh"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.37"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtAvailableforh}"
                            style="@style/txtDMSansmedium18_3" tools:text="@string/lbl_genre"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_indigo_a200_63_radius_14"
                            android:drawablePadding="@dimen/_7pxh"
                            android:drawableStart="@drawable/img_edit_indigo_a200"
                            android:ellipsize="end"
                            android:hint="@string/lbl_change"
                            android:id="@+id/etChangeButton"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_193pxh"
                            android:layout_weight="0.63"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_15pxh"
                            android:paddingStart="@dimen/_15pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@={myProfileVM.myProfileModel.etChangeButtonValue}"
                            android:textAllCaps="true"
                            android:textColorHint="@color/indigo_A200"
                            style="@style/etSolidRounded" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowgenreone"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreOne"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.4"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_15pxh"
                            android:paddingStart="@dimen/_15pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtGenreOne}"
                            style="@style/txtSolidRounded" tools:text="@string/lbl_orchestra"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_weight="0.24"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_5pxv"
                            android:paddingEnd="@dimen/_15pxh"
                            android:paddingStart="@dimen/_15pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtGenreTwo}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_pop"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtGenreThree"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_weight="0.36"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_5pxv"
                            android:paddingEnd="@dimen/_15pxh"
                            android:paddingStart="@dimen/_15pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtGenreThree}"
                            style="@style/txtSolidRounded_2" tools:text="@string/lbl_dangdut"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearTitleBar2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHelloAshfak"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.83"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{myProfileVM.myProfileModel.txtHelloAshfak}"
                        style="@style/txtDMSansmedium24" tools:text="@string/lbl_profile"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
    </FrameLayout>
</layout>
