<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpRowModel" type="com.bangkit.elise.modules.signup.data.model.SignUpRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearPasswordfield"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_3">
        <LinearLayout android:id="@+id/linearRowlock"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_weight="0.88" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLock"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_lock"
                tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtValueOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.88" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{signUpRowModel.txtValueOne}"
                style="@style/txtDMSansbold14" tools:text="@string/lbl_your_password"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageHidden"
            android:layout_height="@dimen/_14pxh"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_129pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="@dimen/_18pxh"
            android:scaleType="fitXY" android:src="@drawable/img_hidden"
            tools:ignore="ContentDescription" tools:src="@drawable/img_hidden"/>
    </LinearLayout>
</layout>
