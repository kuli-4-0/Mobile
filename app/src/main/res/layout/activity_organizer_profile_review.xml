<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="organizerProfileReviewVM" type="com.bangkit.elise.modules.organizerprofilereview.data.viewmodel.OrganizerProfileReviewVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackbackground"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageBackground"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_812pxv"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_background"
            tools:ignore="ContentDescription" tools:src="@drawable/img_background"/>
        <LinearLayout android:id="@+id/linearColumnbackgroundOne"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <ScrollView android:clipToPadding="false"
                android:fillViewport="true" android:gravity="start"
                android:id="@+id/scrollViewScrollview"
                android:layout_height="match_parent"
                android:layout_weight="1" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameOrganizerProfi"
                    android:layout_height="match_parent" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBackgroundOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_812pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_background"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_background"/>
                    <LinearLayout
                        android:id="@+id/linearColumneventorganizer"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_80pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_80pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <ImageView
                            android:id="@+id/imageImageEightyNine"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_96pxh"
                            android:layout_marginEnd="@dimen/_76pxh"
                            android:layout_marginStart="@dimen/_76pxh"
                            android:layout_width="@dimen/_96pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_image89_44x43}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image89_44x43"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEventOrganizer"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_76pxh"
                            android:layout_marginStart="@dimen/_76pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtEventOrganizer}"
                            style="@style/txtDMSansmedium24" tools:text="@string/lbl_pesta_ria"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTotalcalories"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_76pxh"
                            android:layout_marginStart="@dimen/_76pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtTotalcalories}"
                            style="@style/txtDMSansbold14" tools:text="@string/lbl_event_organizer"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRownumberfollowing"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_76pxh"
                            android:layout_marginStart="@dimen/_76pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearColumnnumberfollowing"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.57"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtNumberfollowing"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_17pxh"
                                    android:layout_marginStart="@dimen/_17pxh"
                                    android:layout_width="wrap_content"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtNumberfollowing}"
                                    style="@style/txtDMSansmedium16" tools:text="@string/lbl_350"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtTotalcaloriesOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_7pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtTotalcaloriesOne}"
                                    style="@style/txtDMSansbold14" tools:text="@string/lbl_following"/>
                            </LinearLayout>
                            <View android:background="@color/gray_303"
                                android:id="@+id/lineLineFiftyNine"
                                android:layout_height="@dimen/_32pxv"
                                android:layout_marginBottom="@dimen/_7pxv"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_weight="0.01" android:layout_width="0dp"/>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearColumnnumberfollowers"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_22pxh"
                                android:layout_weight="0.43"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtNumberfollowers"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_16pxh"
                                    android:layout_marginStart="@dimen/_18pxh"
                                    android:layout_width="wrap_content"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtNumberfollowers}"
                                    style="@style/txtDMSansmedium16" tools:text="@string/lbl_346"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtFollowers"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtFollowers}"
                                    style="@style/txtDMSansbold14" tools:text="@string/lbl_followers"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowclock"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_21pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearRowuserplus"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:orientation="horizontal" style="@style/groupStyleindigo_2">
                                <ImageView
                                    android:id="@+id/imageUserplus"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_16pxv"
                                    android:layout_marginStart="@dimen/_35pxh"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_userplus"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_userplus"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtD"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_14pxv"
                                    android:layout_marginEnd="@dimen/_35pxh"
                                    android:layout_marginStart="@dimen/_13pxh"
                                    android:layout_marginTop="@dimen/_14pxv"
                                    android:layout_weight="0.71"
                                    android:layout_width="0dp"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtD}"
                                    style="@style/txtDMSansbold16_4" tools:text="@string/lbl_follow"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearMessages"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_17pxh"
                                android:layout_weight="0.5"
                                android:layout_width="0dp"
                                android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                                <ImageView android:id="@+id/imageClock"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_16pxv"
                                    android:layout_marginStart="@dimen/_25pxh"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_clock_indigo_a200"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock_indigo_a200"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_12pxv"
                                    android:layout_marginEnd="@dimen/_20pxh"
                                    android:layout_marginStart="@dimen/_14pxh"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_weight="0.82"
                                    android:layout_width="0dp"
                                    android:lineSpacingExtra="@dimen/_5pxh"
                                    android:singleLine="true"
                                    android:text="@{organizerProfileReviewVM.organizerProfileReviewModel.txtDOne}"
                                    style="@style/txtDMSansbold16" tools:text="@string/lbl_messages"/>
                            </LinearLayout>
                        </LinearLayout>
                        <com.google.android.material.tabs.TabLayout
                            android:gravity="start"
                            android:id="@+id/tabLayoutGroup1371"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_33pxv"
                            android:layout_width="match_parent"
                            app:tabMode="fixed"
                            app:tabSelectedTextColor="@color/indigo_A200" app:tabTextColor="@color/gray_600"/>
                    </LinearLayout>
                    <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/viewPagerViewpager"
                        android:layout_gravity="bottom|center"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_weight="1" android:layout_width="match_parent"/>
                </FrameLayout>
            </ScrollView>
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_833pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearTitleBar2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <ImageView android:id="@+id/imageMorevertical"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_295pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_morevertical"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
    </FrameLayout>
</layout>
