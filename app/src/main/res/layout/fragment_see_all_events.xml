<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="seeAllEventsVM" type="com.bangkit.elise.modules.seeallevents.data.viewmodel.SeeAllEventsVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSeeAllEvents"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_31pxv"
        android:layout_marginTop="@dimen/_51pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnarrowleft"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearTitleBar2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_weight="0.73"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHelloAshfak"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_weight="0.87"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{seeAllEventsVM.seeAllEventsModel.txtHelloAshfak}"
                            style="@style/txtDMSansmedium24" tools:text="@string/lbl_my_event"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowsearch"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_130pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search_gray_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_900"/>
                        <ImageView android:id="@+id/imageMorevertical"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_1pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_morevertical"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearCard1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_2">
                <FrameLayout android:id="@+id/frameStackOne"
                    android:layout_height="@dimen/_92pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_79pxh" style="@style/groupStyleorange_200cornerRadius">
                    <ImageView android:id="@+id/imageImageEightyOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxh"
                        android:layout_width="@dimen/_79pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_image81}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image81"/>
                    <FrameLayout android:id="@+id/frameStack"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxv" android:layout_width="@dimen/_79pxh">
                        <ImageView
                            android:id="@+id/imageImageEightyFive"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_92pxh"
                            android:layout_width="@dimen/_79pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_image85}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image85"/>
                        <ImageView android:id="@+id/imageImageEightySix"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_92pxh"
                            android:layout_width="@dimen/_79pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_image86}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image86"/>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearEventdesc1"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_57pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.67"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{seeAllEventsVM.seeAllEventsModel.txtTime}"
                        android:textAllCaps="true"
                        style="@style/txtDMSansmedium12" tools:text="@string/msg_1st_may_sat"/>
                    <TextView android:id="@+id/txtImGoingtoSh"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:text="@{seeAllEventsVM.seeAllEventsModel.txtImGoingtoSh}"
                        style="@style/txtDMSansmedium18_2" tools:text="@string/msg_a_virtual_eveni"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerSeeAllEvents"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_20pxh}" tools:itemCount="2" tools:listitem="@layout/row_see_all_events"/>
        </LinearLayout>
    </LinearLayout>
</layout>
