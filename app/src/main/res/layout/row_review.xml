<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="reviewRowModel" type="com.bangkit.elise.modules.organizerprofilereviewfragment.data.model.ReviewRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowellipsefiftytwo"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageEllipseFiftyTwo"
            android:layout_height="@dimen/_34pxh"
            android:layout_marginBottom="@dimen/_93pxv"
            android:layout_width="@dimen/_34pxh"
            android:scaleType="centerCrop" circular="@{true}"
            imageRes="@{@drawable/img_ellipse52}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse52"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnrocksvelkeinje"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.89" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowrocksvelkeinje"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRocksVelkeinje"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.75"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{reviewRowModel.txtRocksVelkeinje}"
                    style="@style/txtDMSansmedium18_1" tools:text="@string/msg_rocks_velkeinje"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFebCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{reviewRowModel.txtFebCounter}"
                    style="@style/txtDMSansbold15_2" tools:text="@string/lbl_10_feb"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowstar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarOne"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarTwo"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarThree"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtCinemasisthe"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_10pxh"
                android:text="@{reviewRowModel.txtCinemasisthe}"
                style="@style/txtDMSansbold15_3" tools:text="@string/msg_cinemas_is_the"/>
        </LinearLayout>
    </LinearLayout>
</layout>
