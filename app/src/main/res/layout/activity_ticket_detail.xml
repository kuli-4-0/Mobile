<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="ticketDetailVM" type="com.bangkit.elise.modules.ticketdetail.data.viewmodel.TicketDetailVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearTicketDetail"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_48pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearTitleBar2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_weight="0.98"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHelloAshfak"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.81"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{ticketDetailVM.ticketDetailModel.txtHelloAshfak}"
                        style="@style/txtDMSansmedium24" tools:text="@string/lbl_detail"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageMorevertical"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_216pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_1pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_morevertical"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameEticketCard"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_503pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_55pxv" android:layout_width="@dimen/_340pxh">
            <View
                android:background="@drawable/rectangle_bg_bluegray_50_38_radius_10"
                android:id="@+id/viewRectangle2Cop"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_45pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_272pxh"/>
            <View
                android:background="@drawable/rectangle_bg_bluegray_50_b2_radius_10"
                android:id="@+id/viewRectangle2CopOne"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_56pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_305pxh"/>
            <FrameLayout android:id="@+id/frameStackbase"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_481pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_340pxh">
                <ImageView android:id="@+id/imageBase"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_481pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_base"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_base"/>
                <LinearLayout android:id="@+id/linearColumnlinetwo"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_26pxv"
                    android:layout_marginEnd="@dimen/_19pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnconcer"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnConcer"
                            android:layout_height="@dimen/_21pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_82pxh"
                            android:paddingBottom="@dimen/_3pxv"
                            android:paddingEnd="@dimen/_23pxh"
                            android:paddingStart="@dimen/_23pxh"
                            android:paddingTop="@dimen/_3pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_concer"
                            android:textAllCaps="false"
                            style="@style/btnRoundedOutline_1" tools:text="@string/lbl_concer"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTittleTicket"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.02"
                            android:singleLine="true"
                            android:text="@{ticketDetailVM.ticketDetailModel.txtTittleTicket}"
                            style="@style/txtDMSansregular18_1" tools:text="@string/msg_international_b2"/>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerTicketDetail"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_36pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                            itemSpace="@{@dimen/_22pxh}"
                            tools:itemCount="2" tools:listitem="@layout/row_ticket_detail"/>
                    </LinearLayout>
                    <View android:background="@color/gray_508"
                        android:id="@+id/lineLineTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_101pxv" android:layout_width="@dimen/_303pxh"/>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imageBarcode"
                        android:layout_height="@dimen/_59pxh"
                        android:layout_marginEnd="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_39pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_barcode"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_barcode"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSerialcode"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{ticketDetailVM.ticketDetailModel.txtSerialcode}"
                        style="@style/txtDMSansregular11" tools:text="@string/msg_9824_0972_1742"/>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameButtonColor"
            android:layout_height="@dimen/_75pxv"
            android:layout_marginTop="@dimen/_98pxv"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearBtnsignup"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_A200">
                <FrameLayout android:id="@+id/frameStackarrowright"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_32pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_33pxh" style="@style/groupStyleindigo_A400cornerRadius">
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </FrameLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtContinue"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_95pxh"
                android:layout_marginStart="@dimen/_95pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="wrap_content"
                android:letterSpacing="0.05" android:singleLine="true"
                android:text="@{ticketDetailVM.ticketDetailModel.txtContinue}"
                android:textAllCaps="true"
                style="@style/txtDMSansmedium16_1" tools:text="@string/msg_verification_ti"/>
        </FrameLayout>
    </LinearLayout>
</layout>
